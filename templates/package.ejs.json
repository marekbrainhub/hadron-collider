{
  "name": "<%-projectName%>",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    <%_ packages.forEach(function(package) { _%>
    "@brainhubeu/<%-package%>": "*",
    <%_ }) _%>
    <%_ if(packages.includes('hadron-express')) { _%>
    "express": "^4.16.3",
    "body-parser": "*",
    <%_ } _%>
    <%_ if(packages.includes('hadron-typeorm')) { _%>
    "mysql": "*",
    <%_ } _%>
    "@brainhubeu/hadron-core": "^1.0.0"
  },
  "devDependencies": {
    <%_ if(linter !== 'none') { _%>
    "eslint": "^5.0.1",
    <%_ } _%>
    <%_ if(linter === 'brainhub') { _%>
    "eslint-config-brainhub": "^1.9.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    <%_ } _%>
    <%_ if(linter === 'airbnb') { _%>
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    <%_ } _%>
    <%_ if(linter === 'standard') { _%>
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    <%_ } _%>
    <%_ if(babel === 'import') { _%>
    "babel-cli": "^6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-register": "6.26.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    <%_ } _%>
    "nodemon": "*"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js"
  }
}
